public class SelectionSort {
    public static void main(String[] args) {
        int[] data = {1,3,9,6,4,5,2};
        System.out.println(sort(data));
    }

    static int[] sort(int[] data) {
        for(i = 0; i < data.length; i ++) {
            for(j = i; j < data.length; j ++) {
                if(data[i] < data[j]) {
                    int buffer = data[i]
                    data[i] = data[j]
                    data[j] = buffer
                }
            }
        }
    }
}